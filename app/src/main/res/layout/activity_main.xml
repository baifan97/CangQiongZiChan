<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/neu_background"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_32"
        android:layout_marginBottom="@dimen/dp_10"
        android:text="苍穹之资产查询"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="@dimen/dp_20" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_weight="1"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/nourladdurl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginBottom="@dimen/dp_20"
                android:background="@drawable/neu_background_card_down_bac"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_145"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_50"
                    android:layout_marginRight="@dimen/dp_20"
                    android:layout_marginBottom="@dimen/dp_30"
                    app:srcCompat="@mipmap/noaddck" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_marginRight="@dimen/dp_20"
                    android:fontFamily="sans-serif"
                    android:lineSpacingExtra="@dimen/dp_10"
                    android:text="大人，您还没有配置资产查询的接口"
                    android:textAlignment="center"
                    android:textColor="@color/huise"
                    android:textSize="@dimen/dp_13" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_marginRight="@dimen/dp_20"
                    android:layout_marginBottom="@dimen/dp_50"
                    android:lineSpacingExtra="@dimen/dp_10"
                    android:text="点此配置接口"
                    android:textAlignment="center"
                    android:textColor="@color/circular_arc_progress_view_first_progress_color"
                    android:textSize="@dimen/dp_13" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/zccxjiekou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/dp_2"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_2"
                android:background="@drawable/neu_background_card_down_bac"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_32"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_2"
                            android:layout_marginTop="@dimen/dp_3"
                            android:text="编辑Saobing地址"
                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dp_18" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                        <Button
                            android:id="@+id/savezcurl"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dp_28"
                            android:layout_marginLeft="@dimen/dp_8"
                            android:layout_marginTop="@dimen/dp_0"
                            android:layout_marginRight="@dimen/dp_8"
                            android:layout_marginBottom="@dimen/dp_10"
                            android:background="@drawable/background_count_down_bac"
                            android:text="编辑"
                            android:textColor="@color/white"
                            android:textSize="@dimen/dp_12" />

                    </LinearLayout>


                </LinearLayout>

                <TextView
                    android:id="@+id/tip3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_0"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:text="Saobing 资产查询的接口应该没有人不认识吧？"
                    android:textColor="@color/huise"
                    android:textSize="@dimen/dp_13" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/zccxanniu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/dp_2"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_2"
                android:background="@drawable/neu_background_card_down_bac"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_32"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_0"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_2"
                        android:layout_marginTop="@dimen/dp_3"
                        android:text="输入你的Cookie"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dp_18" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_3"
                        android:layout_marginTop="@dimen/dp_3"

                        android:text="Pro"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                        android:textColor="@color/yellow1"
                        android:textSize="@dimen/dp_20"
                        android:visibility="gone" />

                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <Switch
                        android:id="@+id/hideCK"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/dp_12"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/dp_14"
                        android:thumb="@drawable/switch_thumb1"
                        android:track="@drawable/switch_track1" />

                    <Button

                        android:layout_width="@dimen/dp_60"
                        android:layout_height="@dimen/dp_26"
                        android:layout_marginLeft="@dimen/dp_25"
                        android:layout_marginTop="@dimen/dp_0"
                        android:layout_marginRight="@dimen/dp_10"
                        android:background="@drawable/background_count_down_bac"

                        android:text="V6.2"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_13"
                        android:visibility="gone" />

                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/inputkuang"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/shape_text_frame">

                    <EditText
                        android:id="@+id/input"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_2"
                        android:layout_marginTop="@dimen/dp_2"
                        android:layout_marginRight="@dimen/dp_2"
                        android:layout_marginBottom="@dimen/dp_0"
                        android:background="@null"
                        android:hint="请输入京东Cookie"
                        android:maxHeight="@dimen/dp_280"
                        android:textColor="@color/black"
                        android:textColorHint="@color/huise"
                        android:textSize="@dimen/dp_14"
                        tools:ignore="TouchTargetSizeCheck" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <Button
                        android:id="@+id/lookupckmonth"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dp_33"
                        android:layout_marginLeft="@dimen/dp_8"
                        android:layout_marginTop="@dimen/dp_6"
                        android:layout_marginRight="@dimen/dp_8"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:background="@drawable/background_count_down_bac"
                        android:text="月资产"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_13" />

                    <Button
                        android:id="@+id/lookupck"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dp_33"
                        android:layout_marginLeft="@dimen/dp_8"
                        android:layout_marginTop="@dimen/dp_6"
                        android:layout_marginRight="@dimen/dp_8"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:background="@drawable/background_count_down_bac"
                        android:text="日资产"
                        android:textColor="@color/white"
                        android:textSize="@dimen/dp_13" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lookupjieguo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/dp_2"
                android:layout_marginTop="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_2"
                android:background="@drawable/neu_background_card_down_bac"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_32"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_0"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_2"
                        android:layout_marginTop="@dimen/dp_3"
                        android:text="查询结果"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dp_18" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <ImageView
                        android:id="@+id/dian"
                        android:layout_width="@dimen/dp_55"
                        android:layout_height="match_parent"
                        app:srcCompat="@mipmap/dian" />

                </LinearLayout>

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1_5"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_marginTop="@dimen/dp_6"
                    android:layout_marginRight="@dimen/dp_8"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="#65E0E0E0" />

                <TextView
                    android:id="@+id/Tip"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_0"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:layout_weight="1"
                    android:text="资产查询大约需要至少 40S，请耐心等待"
                    android:textColor="@color/huise"
                    android:textSize="@dimen/dp_13" />

                <TextView
                    android:id="@+id/lookupdata"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_marginTop="@dimen/dp_3"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:layout_weight="1"
                    android:lineSpacingExtra="@dimen/dp_8"
                    android:text=" "
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_13" />

            </LinearLayout>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_25"
                android:layout_marginRight="@dimen/dp_5"
                android:layout_marginBottom="@dimen/dp_10"
                android:text="Copyright © 2018-2022 WhiteFan All Right Sreserved."
                android:textAlignment="center"
                android:textColor="@color/huise"
                android:textSize="@dimen/dp_11" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_20" />


        </LinearLayout>
    </ScrollView>


</LinearLayout>